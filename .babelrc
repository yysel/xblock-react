{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-jsx-classname-components",
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining",
    "transform-decorators",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    [
      "module-alias",
      [
        {
          "src": "./src/assets",
          "expose": "_assets"
        },
        {
          "src": "./src/blocks",
          "expose": "_blocks"
        },
        {
          "src": "./src/components",
          "expose": "_components"
        },
        {
          "src": "./src/elements",
          "expose": "_elements"
        },
        {
          "src": "./src/layouts",
          "expose": "_layouts"
        },
        {
          "src": "./src/models",
          "expose": "_models"
        },
        {
          "src": "./src/styles",
          "expose": "_styles"
        },
        {
          "src": "./src/tools",
          "expose": "_tools"
        },
        {
          "src": "./src/xblock",
          "expose": "_xblock"
        }
      ]
    ]
  ]
}
